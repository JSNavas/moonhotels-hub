{
  "name": "moonhotels",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "mochapack test/**/*.spec.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vue/test-utils": "^2.4.6",
    "mocha": "^10.7.3",
    "mochapack": "^2.1.4",
    "pinia": "^2.2.2",
    "vue": "^3.2.25",
    "vue-jest": "^3.0.7",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.7",
    "vite": "^2.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(js)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
